
# 2. Determine how many elements in sequence are > their peers above

numbers = [12, 3, 25, 56, 9, 18, 27, 0]
print('Current List: ', numbers)

count = len(numbers)
greaterElementCount = 0

print('Elements greater than their peers above: ')

for i in range(1, count):
    if numbers[i] > numbers[i - 1]:
        print(numbers[i])
        greaterElementCount += 1

print('Total Count: ', greaterElementCount)
